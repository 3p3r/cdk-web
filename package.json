{
  "name": "cdk-web",
  "version": "3.0.2",
  "homepage": "",
  "description": "AWS CDK in your browser",
  "types": "./index.d.ts",
  "main": "./dist/cdk-web.js",
  "keywords": [
    "iac",
    "cdk",
    "aws",
    "aws-cdk",
    "webpack",
    "cloudformation"
  ],
  "scripts": {
    "build": "npm-run-all build:webpack build:compress build:docs build:playground build:postprocess",
    "build:docs": "jsdoc2md --separators webpack/modules/cli.js > docs/cli.md",
    "build:version": "git rev-parse --short HEAD",
    "build:webpack": "npm-run-all build:webpack:*",
    "build:webpack:bundle": "webpack --config cdk-web.webpack.config.js",
    "build:webpack:verify": "tsc  --noEmit index.d.ts",
    "build:compress": "npm-run-all build:compress:*",
    "build:compress:compile": "google-closure-compiler --js=dist/cdk-web.js --js_output_file=dist/cdk-web.min.js --warning_level=QUIET",
    "build:compress:postprocess": "shx rm dist/cdk-web.js && shx mv dist/cdk-web.min.js dist/cdk-web.js && shx cp dist/cdk-web.js public/cdk-web.js",
    "build:playground": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "build:postprocess": "(shx test -d .git && npm-run-all build:postprocess:*) || shx echo no .git",
    "build:postprocess:rename": "shx mv ./build/cdk-web.js ./build/cdk-web-$(npm run -s build:version).js",
    "build:postprocess:replace": "shx sed -i s/cdk-web.js/cdk-web-$(npm run -s build:version).js/g $(shx find ./build/static/js/main.*.js) | shusher",
    "clean": "git clean -xdf -e node_modules",
    "test": "jest --runInBand --ci",
    "dev:playground": "CHOKIDAR_USEPOLLING=true SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "dev:framework": "CDK_WEB_DEBUG=1 DEBUG='CdkWeb*' CHOKIDAR_USEPOLLING=true webpack-dev-server --config cdk-web.webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3p3r/cdk-web.git"
  },
  "author": "Sepehr Laal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3p3r/cdk-web/issues"
  },
  "devDependencies": {
    "@aws-cdk/cloud-assembly-schema": "^2.14.0",
    "@aws-cdk/cloudformation-diff": "^2.14.0",
    "@aws-cdk/cx-api": "^2.14.0",
    "@forevolve/bootstrap-dark": "^2.1.0",
    "@monaco-editor/react": "^4.3.1",
    "@types/estree": "^0.0.51",
    "aws-cdk": "^2.13.0",
    "aws-cdk-lib": "^2.10.0",
    "aws-sdk": "^2.1077.0",
    "bootstrap": "^5.1.3",
    "cdk-assets": "^2.14.0",
    "constructs": "^10.0.52",
    "debug": "^4.3.4",
    "eventemitter2": "^6.4.5",
    "fast-deep-equal": "^3.1.3",
    "github-fork-ribbon-css": "^0.2.3",
    "glob": "^7.2.0",
    "google-closure-compiler": "^20220301.0.0",
    "ignore": "^5.2.0",
    "jest": "^27.4.7",
    "jest-puppeteer": "^6.1.0",
    "jsdoc-to-markdown": "^7.1.1",
    "loader-utils": "^2.0.2",
    "lodash": "^4.17.21",
    "memfs": "^3.4.1",
    "npm-dts": "^1.3.11",
    "npm-run-all": "^4.1.5",
    "os-browserify": "^0.3.0",
    "precinct": "^9.0.0",
    "puppeteer": "^13.1.3",
    "react": "~16",
    "react-bootstrap": "^2.1.2",
    "react-dom": "~16",
    "react-error-overlay": "6.0.9",
    "react-loader-spinner": "^5.1.2",
    "react-resize-detector": "^7.0.0",
    "react-scripts": "~4",
    "schema-utils": "^3.1.1",
    "shusher": "^0.1.1",
    "shx": "^0.3.4",
    "string-hash": "^1.1.3",
    "strip-indent": "^4.0.0",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4",
    "webpack-cli": "~3",
    "webpack-dev-server": "~3"
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "preset": "jest-puppeteer",
    "testTimeout": 120000,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/docs"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
