{
  "name": "cdk-web",
  "version": "1.5.13",
  "homepage": "",
  "description": "AWS CDK in your browser",
  "main": "./dist/cdk-web.js",
  "keywords": [
    "iac",
    "cdk",
    "aws",
    "aws-cdk",
    "webpack",
    "cloudformation"
  ],
  "scripts": {
    "build": "npm-run-all build:webpack build:docs build:playground build:postprocess",
    "build:docs": "jsdoc2md --separators cdk-web-cli.js > docs/cli.md",
    "build:version": "git rev-parse --short HEAD",
    "build:webpack": "webpack --config cdk-web.webpack.config.js",
    "build:playground": "react-scripts build",
    "build:postprocess": "(shx test -d .git && npm-run-all build:postprocess:*) || shx echo no .git",
    "build:postprocess:rename": "shx mv ./build/cdk-web.js ./build/cdk-web-$(npm run -s build:version).js",
    "build:postprocess:replace": "shx sed -i s/cdk-web.js/cdk-web-$(npm run -s build:version).js/g $(shx find ./build/static/js/main.*.js) | shusher",
    "clean": "git clean -xvf -e node_modules",
    "test": "jest --runInBand --ci",
    "playground": "react-scripts start",
    "start": "CDK_WEB_DEBUG=1 webpack-dev-server --config cdk-web.webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3p3r/cdk-web.git"
  },
  "author": "Sepehr Laal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3p3r/cdk-web/issues"
  },
  "devDependencies": {
    "@aws-cdk/cloud-assembly-schema": "^2.14.0",
    "@aws-cdk/cloudformation-diff": "^2.14.0",
    "@aws-cdk/cx-api": "^2.14.0",
    "@forevolve/bootstrap-dark": "^2.1.0",
    "@monaco-editor/react": "^4.3.1",
    "@types/estree": "^0.0.51",
    "aws-cdk": "^2.13.0",
    "aws-cdk-lib": "^2.10.0",
    "aws-sdk": "^2.1077.0",
    "bootstrap": "^5.1.3",
    "cdk-assets": "^2.14.0",
    "constructs": "^10.0.52",
    "github-fork-ribbon-css": "^0.2.3",
    "idempotent-babel-polyfill": "^7.4.4",
    "jest": "^27.4.7",
    "jest-puppeteer": "^6.1.0",
    "jsdoc-to-markdown": "^7.1.1",
    "lodash": "^4.17.21",
    "memfs": "^3.4.1",
    "npm-dts": "^1.3.11",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "os-browserify": "^0.3.0",
    "puppeteer": "^13.1.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-loader-spinner": "^5.1.2",
    "react-resize-detector": "^7.0.0",
    "react-scripts": "5.0.0",
    "shelljs": "^0.8.5",
    "shusher": "^0.1.1",
    "shx": "^0.3.4",
    "string-replace-loader": "^2.3.0",
    "strip-indent": "^4.0.0",
    "typescript": "^4.5.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "web-vitals": "^2.1.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3"
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "preset": "jest-puppeteer",
    "testTimeout": 120000
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
